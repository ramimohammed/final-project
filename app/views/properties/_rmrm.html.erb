
<h3>Assets for the project</h3>



<ul>
  <%= @property.rmrms.each do |rmrm| %>
    <li>
        <%= rmrm.file %>
    </li>
  <% end %>


<% form_for [ @property, @property.rmrms.build ], multipart: true do |f| %>
<p>
  <% f.file_field :file %>
   <%f.hidden_field :property_id, :value => params[:id] %>
  <% f.submit %>
</p>
 
<% end %>


